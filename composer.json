{
    "name": "spatie/global-laravel-remote",
    "description": "Execute artisan commands on your server",
    "keywords": [
        "spatie",
        "laravel",
        "global-laravel-remote"
    ],
    "homepage": "https://github.com/spatie/global-laravel-remote",
    "license": "MIT",
    "authors": [
        {
            "name": "Francisco Madeira",
            "role": "Developer"
        },
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel-zero/framework": "^9.2",
        "nunomaduro/termwind": "^1.14",
        "spatie/laravel-remote": "^1.3",
        "spatie/valuestore": "^1.3"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "pestphp/pest": "^1.21",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "build": "php global-laravel-remote app:build global-laravel-remote",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["global-laravel-remote"]
}
