{
    "name": "spatie/global-laravel-remote",
    "description": "Execute artisan commands on your server",
    "keywords": [
        "spatie",
        "laravel",
        "global-laravel-remote"
    ],
    "homepage": "https://github.com/spatie/global-laravel-remote",
    "license": "MIT",
    "authors": [
        {
            "name": "Francisco Madeira",
            "role": "Developer"
        },
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel-zero/framework": "^10.2",
        "laravel/prompts": "^0.1.15",
        "nunomaduro/termwind": "^1.15.1",
        "spatie/laravel-remote": "^1.3",
        "spatie/valuestore": "^1.3"
    },
    "require-dev": {
        "larastan/larastan": "^2.9",
        "laravel/pint": "^1.14",
        "mockery/mockery": "^1.6",
        "pestphp/pest": "^2.22"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["global-laravel-remote"]
}
